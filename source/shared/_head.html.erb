<meta charset="utf-8">
<!-- Always force latest IE rendering engine or request Chrome Frame -->
<meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
<!-- Responsive -->
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width, initial-scale=1">
<!-- Use title if it's in the page YAML frontmatter -->
<title><%= current_page.data.title %></title>
<meta name="description" content="<%= current_page.data.description %>">

<!-- Open Graph FB -->
<meta property="og:title" content="<%= current_page.data.title %>">
<meta property="og:type" content="website">
<meta property="og:description" content="<%= current_page.data.description %>">
<meta property="og:url" content="http://www.cforgood.com<%= current_page.url %>">
<meta property="og:image" content="http://www.cforgood.com/<%= image_path current_page.data.thumbnail %>">
<meta property="og:site_name" content="CforGood">

<!-- Twitter card data -->
<meta name="twitter:card" content="summary_large_image">
<meta name="twitter:site" content="@CforGood">
<meta name="twitter:title" content="<%= current_page.data.title %>">
<meta name="twitter:description" content="<%= current_page.data.description %>">
<meta name="twitter:creator" content="@CforGood">
<meta name="twitter:image:src" content="http://www.cforgood.com/<%= image_path current_page.data.thumbnail %>">

<!-- Schema.org markup for Google+ -->
<meta itemprop="name" content="<%= current_page.data.title %>">
<meta itemprop="description" content="<%= current_page.data.description %>">
<meta itemprop="image" content="http://www.cforgood.com/<%= image_path current_page.data.thumbnail %>">

<%= feed_tag :atom, "#{blog.options.prefix.to_s}/feed.xml", title: "Atom Feed" %>



<%= stylesheet_link_tag "all" %>
<%= javascript_include_tag  "all" %>
<!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
<!-- Leave those next 4 lines if you care about users using IE8 -->
<!--[if lt IE 9]>
  <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
  <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
<![endif]-->